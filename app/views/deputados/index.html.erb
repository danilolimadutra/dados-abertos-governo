<p id="notice"><%= notice %></p>

<h1>Deputados</h1>

<table>
  <thead>
    <tr>
      <th>Cod dep</th>
      <th>Uri</th>
      <th>Nome</th>
      <th>Sigla partido</th>
      <th>Uri partido</th>
      <th>Sigla uf</th>
      <th>Id legislatura</th>
      <th>Url foto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deputados.each do |deputado| %>
      <tr>
        <td><%= deputado.cod_dep %></td>
        <td><%= deputado.uri %></td>
        <td><%= deputado.nome %></td>
        <td><%= deputado.sigla_partido %></td>
        <td><%= deputado.uri_partido %></td>
        <td><%= deputado.sigla_uf %></td>
        <td><%= deputado.id_legislatura %></td>
        <td><%= deputado.url_foto %></td>
        <td><%= link_to 'Show', deputado %></td>
        <td><%= link_to 'Edit', edit_deputado_path(deputado) %></td>
        <td><%= link_to 'Destroy', deputado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Deputado', new_deputado_path %>
